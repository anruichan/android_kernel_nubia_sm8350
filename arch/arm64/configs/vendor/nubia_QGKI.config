# nubia QGKI config

# Battery
CONFIG_FEATURE_NUBIA_BATTERY_1S_2S=y

# Bypass acl
CONFIG_NUBIA_BYPASS_ACL=y

# Cgroup
CONFIG_NUBIA_CGF_NOTIFY_EVENT=y

# Debug
CONFIG_CLEANCACHE=n
CONFIG_DEBUG_LIST=n
CONFIG_DEBUG_MEMORY_INIT=n
CONFIG_DEBUG_STACK_USAGE=n
CONFIG_HAVE_DEBUG_KMEMLEAK=n
CONFIG_MSM_CORE_HANG_DETECT=n

# Display
CONFIG_NUBIA_DP=y
CONFIG_NUBIA_USB_DP_MODULES=y

# exFAT
CONFIG_EXFAT_FS=y
CONFIG_EXFAT_DEFAULT_IOCHARSET="utf8"
CONFIG_EXFAT_VIRTUAL_XATTR=y
CONFIG_EXFAT_VIRTUAL_XATTR_SELINUX_LABEL="u:object_r:exfat:s0"

# Fan
CONFIG_NUBIA_FAN=y
CONFIG_FAN_CORE=y
CONFIG_FAN_UPDATA=y

# Fingerprint
CONFIG_NUBIA_FINGERPRINT=y
CONFIG_NUBIA_FP_GOODIX=y
CONFIG_NUBIA_FP_GOODIX_GW9658=y

# Haptic
CONFIG_AW8695_HAPTIC=y
CONFIG_AW8697_HAPTIC=y

# Hardware version
CONFIG_NUBIA_HW_VERSION=y
CONFIG_NUBIA_HW_CPU_SERIAL_NUM=y
CONFIG_NUBIA_HW_CONFIG_BY_GPIO=y

# Keyboard
CONFIG_NUBIA_ADD_POWERKEY_DEBOUNCE=y
CONFIG_NUBIA_CYPRESS_KEYBOARD_GAMEFIRE=y
CONFIG_NUBIA_CYPRESS_KEYBOARD_LEFT_GAMEFIRE=y

# Leds
CONFIG_NUBIA_LED=y
CONFIG_NUBIA_LED_AW22XXX=y
CONFIG_ZTEMT_BREATH_LEDS=y

# Log
CONFIG_NBLOG=y
CONFIG_NUBIA_LOG=y

# Memory
CONFIG_HOTPLUG_SIZE_BITS=n
CONFIG_MEMORY_HOTPLUG=n
CONFIG_MEMORY_HOTPLUG_DEFAULT_ONLINE=n
CONFIG_MEMORY_HOTPLUG_MOVABLE_NODE=n
CONFIG_MEMORY_HOTREMOVE=n
CONFIG_QCOM_MEM_OFFLINE=n
CONFIG_OVERRIDE_MEMORY_LIMIT=n

# NFC
CONFIG_NUBIA_ESE=y
CONFIG_NFC=y
CONFIG_SEC_NFC=y
CONFIG_SEC_NFC_PRODUCT_N5=y
CONFIG_SEC_NFC_IF_I2C=y

# Nubia drivers
CONFIG_NUBIA_DRIVERS=y

# NX669J Support
CONFIG_BOARD_NUBIA_NX669J=y

# Perf
CONFIG_SERIAL_MSM_GENI_CONSOLE=n

# Power
CONFIG_ZTEMT_POWER_DEBUG=y

# Pstore
CONFIG_PSTORE=y
CONFIG_PSTORE_CONSOLE=y
CONFIG_PSTORE_PMSG=y
CONFIG_PSTORE_RAM=y

# Sensor
CONFIG_NUBIA_SENSORS=y
CONFIG_NUBIA_SENSORS_STK3337=y
CONFIG_NUBIA_SENSORS_SENS=y
CONFIG_NUBIA_SENSORS_SENS_EN=y

# Touchscreen
CONFIG_NUBIA_TOUCH=y
CONFIG_TOUCHSCREEN_GOODIX_BRL=y
CONFIG_TOUCHSCREEN_GOODIX_BRL_I2C=y
CONFIG_TOUCHSCREEN_GOODIX_TS_TOOLS=y
CONFIG_TOUCHSCREEN_GOODIX_TS_GESTURE=y
CONFIG_TOUCHSCREEN_SYNAPTICS_DSX=y
CONFIG_TOUCHSCREEN_SYNAPTICS_DSX_I2C=y
CONFIG_TOUCHSCREEN_SYNAPTICS_DSX_CORE=y
CONFIG_TOUCHSCREEN_SYNAPTICS_DSX_FW_UPDATE=y
CONFIG_TOUCHSCREEN_SYNAPTICS_DSX_FW_UPDATE_EXTRA_SYSFS=y
CONFIG_TOUCHSCREEN_SYNAPTICS_DSX_RMI_DEV=y
CONFIG_TOUCHSCREEN_SYNAPTICS_DSX_TEST_REPORTING=y

# USB
CONFIG_NUBIA_USB_DISABLE_LINK_PM=y
CONFIG_NUBIA_USB_TEST=y
